<div id="<%= dom_id category_rule %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= category_rule.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Pattern:</strong>
    <%= category_rule.pattern %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Priority:</strong>
    <%= category_rule.priority %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Parent:</strong>
    <%= category_rule.parent_id %>
  </div>
</div>
